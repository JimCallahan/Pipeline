mkinstalldirs = $(SHELL) $(top_srcdir)/config/mkinstalldirs


#--------------------------------------------------------------------------------------------#
#   V A R I A B L E S                                                                        #
#--------------------------------------------------------------------------------------------#

  BUILDER_VERSION = $(notdir $(srcdir))
     BUILDER_NAME = $(notdir $(patsubst %/$(BUILDER_VERSION),%,$(srcdir)))
     BUILDER_PATH = us/temerity/pipeline/builder/$(BUILDER_NAME)/$(BUILDER_VERSION)

           subdir = src/java/$(BUILDER_PATH)

           TARGET = $(BUILDER_NAME).jar

       JAVA_PATHS = -d ../../../../../.. -classpath ../../../api.jar		

     JAVA_SOURCES = $(shell find $(srcdir) -type f -name "*.java")
     JAVA_CLASSES = $(patsubst $(srcdir)/%.java,%.class,$(JAVA_SOURCES))

      BUILDER_DIR = $(ROOT_INST_DIR)/builders/$(BUILDER_PATH)
      EXAMPLE_DIR = $(ROOT_INST_DIR)/examples/$(BUILDER_PATH)



#--------------------------------------------------------------------------------------------#
#   R U L E S                                                                                #
#--------------------------------------------------------------------------------------------#

all: Makefile java-all $(TARGET) 

$(TARGET): java-all
	(cd ../../../../../..; \
         $(JAR) cvf $(BUILDER_PATH)/$@ $(BUILDER_PATH)/*.class $(BUILDER_PATH)/stages/*.class)

java-all: $(JAVA_SOURCES)
	$(JAVA_C) $(JAVA_C_NODEP_FLAGS) $(JAVA_PATHS) $(JAVA_SOURCES)
	touch $@
	rm -f $(top_builddir)/docs/javadoc/docs-all

%.java: %.jj
	$(JAVA_CC) $<

check:


#--------------------------------------------------------------------------------------------#

install: all installdirs install-sources install-target

installdirs:
	$(mkinstalldirs) $(DESTDIR)$(EXAMPLE_DIR)
	$(mkinstalldirs) $(DESTDIR)$(EXAMPLE_DIR)/stages
	$(mkinstalldirs) $(DESTDIR)$(BUILDER_DIR)

install-target: $(TARGET)
	$(INSTALL) -m 644 $< $(DESTDIR)$(BUILDER_DIR);

install-sources: $(JAVA_SOURCES)
	@list='$^'; for p in $$list; do \
          f=`echo $$p | sed -e 's:$(srcdir)/::g'`; \
          if test -f $$p; then \
            echo $(INSTALL) -m 444 $$p $(DESTDIR)$(EXAMPLE_DIR)/$$f; \
            $(INSTALL) -m 444 $$p $(DESTDIR)$(EXAMPLE_DIR)/$$f; \
          fi; \
         done



#--------------------------------------------------------------------------------------------#

uninstall: uninstall-jar

uninstall-target: $(TARGET)
	rm -f $(DESTDIR)$(BUILDER_DIR)/$<


#--------------------------------------------------------------------------------------------#

clean: clean-jar clean-class 

clean-jar:
	rm -f $(TARGET)

clean-class:
	rm -f *.class
	rm -f java-all


#--------------------------------------------------------------------------------------------#

PHONY: installdirs



#--------------------------------------------------------------------------------------------#

Makefile:  $(srcdir)/Makefile.in  $(top_builddir)/config.status
	cd $(top_builddir) && $(SHELL) ./config.status $(subdir)/$@

