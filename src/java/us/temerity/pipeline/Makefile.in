	(cd ../../..; \
	 $(JAVA) -cp us/temerity/pipeline/plugin/$(UTIL_JAR):us/temerity/pipeline/$(API_JAR) \
	         -Dpipeline.ostype=Unix -Dpipeline.archtype=x86_64 -Dpipeline.buildtype=Debug \
		 us.temerity.pipeline.build.PluginInstallUtil \
		 us/temerity/pipeline/plugin/plugin-all \
		 $(DESTDIR)$(ROOT_INST_DIR)/plugins/ \
		 $(DESTDIR)/plugins/required)
	(cd ../../..; \
         for ppath in `cat us/temerity/pipeline/plugin/plugin-extra`; \
         do \
           dir=$(DESTDIR)$(ROOT_INST_DIR)/plugins/`dirname $$ppath`; \
           echo $(mkinstalldirs) $$dir; \
           $(mkinstalldirs) $$dir; \
           echo $(INSTALL) -m 644 $$ppath $$dir; \
           $(INSTALL) -m 644 $$ppath $$dir; \
         done)
	$(mkinstalldirs) $(DESTDIR)$(ROOT_INST_DIR)/plugins-extra/us/temerity/pipeline/plugin
