/* common states */ 
<IGNORE_STATE> 
TOKEN :
{
  <IGNORED: (~[])+>
}


/* boolean arguments */ 
<BOOLEAN_SEP> 
TOKEN :
{
  <EQ1:                   "=">   : BOOLEAN_STATE
| <MISSING_BOOLEAN_ARG: ~["="]>  : IGNORE_STATE
}

<BOOLEAN_STATE> 
TOKEN :
{
  <TRUE:            ["t","T"] ["r","R"] ["u","U"] ["e","E"]>            : OPTIONS
| <FALSE:           ["f","F"] ["a","A"] ["l","L"] ["s","S"] ["e","E"]>  : OPTIONS
| <MISSING_BOOLEAN: ["\0"]>                                             : IGNORE_STATE
| <ILLEGAL_BOOLEAN: (~["\0"])+>                                         : IGNORE_STATE
}


