void 
LinkFrameOffset()
  throws PipelineException :  
{
  Token t;
}
{
  t=<OFFSET> 
  {
    try {
      pLinkFrameOffset = new Integer(t.image);
      pLinkRelationship = LinkRelationship.OneToOne;
    }
    catch(NumberFormatException ex) {
      throw new PipelineException(ex.getMessage());
    }
  }
}


