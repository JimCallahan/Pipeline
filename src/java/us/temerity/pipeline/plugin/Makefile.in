#--------------------------------------------------------------------------------------------#
#   V A R I A B L E S                                                                        #
#--------------------------------------------------------------------------------------------#

          TARGET = plugins.jar


      JAVA_PATHS = -d ../../../.. -classpath ../api.jar		

    JAVA_SOURCES = $(shell ls $(srcdir)/*.java)
    JAVA_CLASSES = $(shell ls *.class)

      PLUGIN_DIR = $(ROOT_INST_DIR)/plugins/us/temerity/pipeline/plugin


#--------------------------------------------------------------------------------------------#
#   R U L E S                                                                                #
#--------------------------------------------------------------------------------------------#

all: Makefile java-all $(TARGET) 

$(TARGET): java-all
	(cd ../../../..; \
         $(JAR) cvf us/temerity/pipeline/plugin/$@ us/temerity/pipeline/plugin/*.class)

java-all: $(JAVA_SOURCES)
	$(JAVA_C) $(JAVA_C_FLAGS) $(JAVA_PATHS) $(JAVA_SOURCES)
	touch $@
	rm -f $(top_builddir)/docs/javadoc/docs-all

%.java: %.jj
	$(JAVA_CC) $<

check:

#--------------------------------------------------------------------------------------------#

install: all installdirs install-classes

install-classes: $(JAVA_CLASSES)
	@list='$^'; for p in $$list; do \
          f=`basename $$p`; \
	  if test -f $$p; then \
            echo $(INSTALL) -m 444 $$p $(DESTDIR)$(PLUGIN_DIR)/$$f; \
            $(INSTALL) -m 444 $$p $(DESTDIR)$(PLUGIN_DIR)/$$f; \
          fi; \
         done


installdirs:
	$(mkinstalldirs) $(DESTDIR)$(PLUGIN_DIR)


#--------------------------------------------------------------------------------------------#

uninstall: uninstall-jar

uninstall-classes: $(JAVA_CLASSES)
	@list='$^'; for p in $$list; do \
          f=`basename $$p`; \
	  if test -f $$p; then \
            echo rm -f $(DESTDIR)$(PLUGIN_DIR)/$$f; \
            rm -f $(DESTDIR)$(PLUGIN_DIR)/$$f; \
          fi; \
         done


#--------------------------------------------------------------------------------------------#

clean: clean-jar clean-class 

clean-jar:
	rm -f $(TARGET)

clean-class:
	rm -f *.class
	rm -f java-all


#--------------------------------------------------------------------------------------------#

PHONY: installdirs



#--------------------------------------------------------------------------------------------#

Makefile:  $(srcdir)/Makefile.in  $(top_builddir)/config.status
	cd $(top_builddir) && $(SHELL) ./config.status $(subdir)/$@

