DIST_COMMON = Makefile.in
subdir = src/java/us/temerity/pipeline/plugin/v2_0_11
mkinstalldirs = $(SHELL) $(top_srcdir)/config/mkinstalldirs


#--------------------------------------------------------------------------------------------#
#   V A R I A B L E S                                                                        #
#--------------------------------------------------------------------------------------------#

  PLUGIN_VERSION = $(notdir $(srcdir))

          TARGET = plugins_$(PLUGIN_VERSION).jar


      JAVA_PATHS = -d ../../../../.. -classpath ../../api.jar		

    JAVA_SOURCES = $(shell find $(srcdir) -type f -name "*.java")
    JAVA_CLASSES = $(patsubst $(srcdir)/%.java,%.class,$(JAVA_SOURCES))

      PLUGIN_DIR = $(ROOT_INST_DIR)/plugins/us/temerity/pipeline/plugin/$(PLUGIN_VERSION)
     EXAMPLE_DIR = $(ROOT_INST_DIR)/examples/us/temerity/pipeline/plugin/$(PLUGIN_VERSION)



#--------------------------------------------------------------------------------------------#
#   R U L E S                                                                                #
#--------------------------------------------------------------------------------------------#

all: Makefile java-all $(TARGET) 

$(TARGET): java-all
	(cd ../../../../..; \
         $(JAR) cvf us/temerity/pipeline/plugin/$(PLUGIN_VERSION)/$@ \
                $(patsubst %,us/temerity/pipeline/plugin/$(PLUGIN_VERSION)/%,$(JAVA_CLASSES)))

java-all: $(JAVA_SOURCES)
	$(JAVA_C) $(JAVA_C_FLAGS) $(JAVA_PATHS) $(JAVA_SOURCES)
	touch $@
	rm -f $(top_builddir)/docs/javadoc/docs-all

%.java: %.jj
	$(JAVA_CC) $<

check:


#--------------------------------------------------------------------------------------------#

install: all installdirs install-sources install-classes 

install-classes: $(JAVA_CLASSES)
	@list='$^'; for p in $$list; do \
          if test -f $$p; then \
            echo $(INSTALL) -m 644 $$p $(DESTDIR)$(PLUGIN_DIR); \
            $(INSTALL) -m 644 $$p $(DESTDIR)$(PLUGIN_DIR); \
          fi; \
         done

installdirs:
	$(mkinstalldirs) $(DESTDIR)$(EXAMPLE_DIR)
	$(mkinstalldirs) $(DESTDIR)$(PLUGIN_DIR)

install-sources: $(JAVA_SOURCES)
	@list='$^'; for p in $$list; do \
          f=`echo $$p | sed -e 's:$(srcdir)/::g'`; \
          if test -f $$p; then \
            echo $(INSTALL) -m 444 $$p $(DESTDIR)$(EXAMPLE_DIR); \
            $(INSTALL) -m 444 $$p $(DESTDIR)$(EXAMPLE_DIR); \
          fi; \
         done



#--------------------------------------------------------------------------------------------#

uninstall: uninstall-jar

uninstall-classes: $(JAVA_CLASSES)
	@list='$^'; for p in $$list; do \
          f=`basename $$p`; \
	  if test -f $$p; then \
            echo rm -f $(DESTDIR)$(PLUGIN_DIR)/$$f; \
            rm -f $(DESTDIR)$(PLUGIN_DIR)/$$f; \
          fi; \
         done


#--------------------------------------------------------------------------------------------#

clean: clean-jar clean-class 

clean-jar:
	rm -f $(TARGET)

clean-class:
	rm -f *.class
	rm -f java-all


#--------------------------------------------------------------------------------------------#

PHONY: installdirs



#--------------------------------------------------------------------------------------------#

Makefile:  $(srcdir)/Makefile.in  $(top_builddir)/config.status
	cd $(top_builddir) && $(SHELL) ./config.status $(subdir)/$@

