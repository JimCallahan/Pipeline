 TEST_CLASS_PATH = ".:../all.jar:../plugin/plugins.jar:tests.jar"

    LOGGER_PROPS = $(srcdir)/../../../../share/logger.properties
         GC_OPTS = #-Xmx256m -Xloggc:garbage-collection.log
      DEBUG_OPTS = -Xdebug -Xrunjdwp:transport=dt_socket,server=y,suspend=n,address=43000


#--------------------------------------------------------------------------------------------#

           TESTS = Math

#             TESTS = CheckSum \
#                     DNotify \
#                     FileSeq \
#                     FileMgr \
#                     Glue \
#                     NativeFileSys \
#                     NodeID \
#                     MasterMgr \
#                     NodeModVersion \
#                     SubProcess \
#                     Toolsets \
#                    VersionID

         CLASSES = $(patsubst %,Test%App.class,$(TESTS))



       INT_TESTS = Plugins \

     INT_CLASSES = $(patsubst %,Test%App.class,$(INT_TESTS))



          TARGET = tests.jar


      JAVA_PATHS = -d . -classpath "../../../..:$(srcdir)/../../../.."

    JAVA_SOURCES = $(shell ls *.java $(srcdir)/*.java)


#--------------------------------------------------------------------------------------------#
#   R U L E S                                                                                #
#--------------------------------------------------------------------------------------------#

all: Makefile $(TARGET)

$(TARGET): java-all
	(cd ../../../..; \
         $(JAR) cvf us/temerity/pipeline/tests/$@ us/temerity/pipeline/tests/*.class)

java-all: $(JAVA_SOURCES)
	$(JAVA_C) $(JAVA_C_FLAGS) $(JAVA_PATHS) $(JAVA_SOURCES)
	touch $@


#--------------------------------------------------------------------------------------------#

check: all check-each 

check-each: $(CLASSES) clean-data
#	cp -r $(srcdir)/data data
	@failed=0; all=0; \
         banner1="========================================================================"; \
         banner2="------------------------------------------------------------------------"; \
         list='$(CLASSES)'; for p in $$list; do \
          c=`basename $$p | sed -e 's/.class//'`; \
          if test -f $$p; then \
            echo $$banner1; \
            echo; \
            echo "  TESTING: "$$c"..."; \
            echo; \
            echo env LD_LIBRARY_PATH=./native:$$LD_LIBRARY_PATH \
                 $(JAVA) -Djava.util.logging.config.file=$(LOGGER_PROPS) $(GC_OPTS) \
                         $(DEBUG_OPTS) -cp $(TEST_CLASS_PATH) $(JAVA_RUNTIME_FLAGS) $$c; \
            if env LD_LIBRARY_PATH=./native:$$LD_LIBRARY_PATH \
                   $(JAVA) -Djava.util.logging.config.file=$(LOGGER_PROPS) $(GC_OPTS) \
                           $(DEBUG_OPTS) -cp $(TEST_CLASS_PATH) $(JAVA_RUNTIME_FLAGS) $$c; \
            then \
              all=`expr $$all + 1`; \
              echo; \
              echo $$banner2; \
              echo "PASS: $$c"; \
            else \
              all=`expr $$all + 1`; \
              failed=`expr $$failed + 1`; \
              echo; \
              echo $$banner2; \
              echo "FAIL: $$c"; \
            fi; \
          fi; \
         done; \
         echo $$banner1; \
         if test "$$failed" -eq 0; then \
           echo "All $$all tests passed"; \
         else \
           echo "$$failed of $$all tests failed"; \
         fi; \
         echo $$banner1; \
         test "$$failed" -eq 0


check-interactive: all check-each-interactive 

check-each-interactive: $(INT_CLASSES) ../api.jar
	rm -rf data
	cp -r $(srcdir)/data data
	@failed=0; all=0; \
         banner1="========================================================================"; \
         banner2="------------------------------------------------------------------------"; \
         list='$(INT_CLASSES)'; for p in $$list; do \
          c=`basename $$p | sed -e 's/.class//'`; \
          if test -f $$p; then \
            echo $$banner1; \
            echo; \
            echo "  TESTING: "$$c"..."; \
            echo; \
            echo env LD_LIBRARY_PATH=./native:$$LD_LIBRARY_PATH \
                 $(JAVA) -Djava.util.logging.config.file=$(LOGGER_PROPS) \
                         -cp $(TEST_CLASS_PATH) $(JAVA_RUNTIME_FLAGS) $$c; \
            if env LD_LIBRARY_PATH=./native:$$LD_LIBRARY_PATH \
                   $(JAVA) -Djava.util.logging.config.file=$(LOGGER_PROPS) \
                           -cp $(TEST_CLASS_PATH) $(JAVA_RUNTIME_FLAGS) $$c; then \
              all=`expr $$all + 1`; \
              echo; \
              echo $$banner2; \
              echo "PASS: $$c"; \
            else \
              all=`expr $$all + 1`; \
              failed=`expr $$failed + 1`; \
              echo; \
              echo $$banner2; \
              echo "FAIL: $$c"; \
            fi; \
          fi; \
         done; \
         echo $$banner1; \
         if test "$$failed" -eq 0; then \
           echo "All $$all tests passed"; \
         else \
           echo "$$failed of $$all tests failed"; \
         fi; \
         echo $$banner1; \
         test "$$failed" -eq 0


