#--------------------------------------------------------------------------------------------#
#   V A R I A B L E S                                                                        #
#--------------------------------------------------------------------------------------------#

  EXTERNAL_DIRS = j2se/1.5.0/docs/api \
                  products/java-media/3D/forDevelopers/J3D_1_3_API/j3dapi \
                  products/java-media/jai/forDevelopers/jai-imageio-1_0-docs \
                  products/java-media/jai/forDevelopers/jai-apidocs            

  EXTERNAL_URLS = $(patsubst %,-link http://java.sun.com/%,$(EXTERNAL_DIRS))


     LOCAL_DIRS = /usr/java/jdk1.5.0/docs/api

     LOCAL_URLS = $(patsubst %,-link file://%,$(LOCAL_DIRS))


  PLUGIN_SOURCES = $(shell find $(top_srcdir)/src/java/us/temerity/pipeline/plugin \
                                -type f -name "*.java")
     PLUGIN_DIRS = $(patsubst $(top_srcdir)/src/java/%.java,%,$(PLUGIN_SOURCES))
 PLUGIN_PACKAGES = $(sort $(subst /,.,$(patsubst %/,%,$(dir $(PLUGIN_DIRS)))))



#  MAIN_PACKAGES = us.temerity.pipeline \
#                  us.temerity.pipeline.bootstrap \
#                  us.temerity.pipeline.core \
#                  us.temerity.pipeline.glue \
#                  us.temerity.pipeline.message \
#                  us.temerity.pipeline.ui \
#                  us.temerity.pipeline.toolset

  MAIN_PACKAGES = us.temerity.pipeline \
                  us.temerity.pipeline.glue \
                  us.temerity.pipeline.toolset

       PACKAGES = $(MAIN_PACKAGES) $(PLUGIN_PACKAGES)

   PACKAGE_HTML = $(patsubst %,$(top_srcdir)/src/java/%/package.html,\
                             $(subst .,/,$(PACKAGES)))


   JAVADOC_OPTS = -sourcepath $(top_srcdir)/src/java \
                  -classpath $(top_builddir)/src/java \
                  -d javadoc -source 1.5 -use $(EXTERNAL_URLS) 

#--------------------------------------------------------------------------------------------#
#   R U L E S                                                                                #
#--------------------------------------------------------------------------------------------#

all: javadoc.tar

javadoc.tar: docs-all
	tar -cvf $@ javadoc

docs-all: $(PACKAGE_HTML)
	$(JAVADOC) $(JAVADOC_OPTS) $(PACKAGES) \
           $(top_builddir)/src/java/us/temerity/pipeline/PackageInfo.java
#           $(top_builddir)/src/java/us/temerity/pipeline/ui/UserPrefs.java \
#           $(top_builddir)/src/java/us/temerity/pipeline/ui/JUserPrefsDialog.java
	touch $@


#--------------------------------------------------------------------------------------------#

install: all installdirs
	(docdir=`pwd`; \
         cd $(DESTDIR)$(ROOT_INST_DIR)/share/docs; \
         tar -xvf $$docdir/javadoc.tar)

PHONY: installdirs
installdirs:
	$(mkinstalldirs) $(DESTDIR)$(ROOT_INST_DIR)/share/docs


#--------------------------------------------------------------------------------------------#

uninstall: 

uninstall-jar: 
	rm -f $(DESTDIR)$(ROOT_INST_DIR)/share/docs/javadoc


#--------------------------------------------------------------------------------------------#

check: all


#--------------------------------------------------------------------------------------------#

clean: 
	rm -rf javadoc
	rm -f javadoc.tar
	rm -f docs-all


#--------------------------------------------------------------------------------------------#

Makefile: $(srcdir)/Makefile.in  $(top_builddir)/config.status
	cd $(top_builddir) && $(SHELL) ./config.status $(subdir)/$@

