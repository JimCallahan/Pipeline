#--------------------------------------------------------------------------------------------#
#   V A R I A B L E S                                                                        #
#--------------------------------------------------------------------------------------------#

  EXTERNAL_URLS = \
      -linkoffline \
        http://java.sun.com/j2se/1.5.0/docs/api \
        $(srcdir)/package-lists/jdk \
      -linkoffline \
        http://java.sun.com/products/java-media/jai/forDevelopers/jai-imageio-1_0-docs \
        $(srcdir)/package-lists/jai-imageio \
      -linkoffline \
        http://java.sun.com/products/java-media/jai/forDevelopers/jai-apidocs \
        $(srcdir)/package-lists/jai \
      -linkoffline \
        ../../../jogl/docs \
        $(srcdir)/package-lists/jogl


     LOCAL_DIRS = /usr/java/jdk1.5.0/docs/api

     LOCAL_URLS = $(patsubst %,-link file://%,$(LOCAL_DIRS))


 LEGACY_PLUGINS = $(BUILD_LEGACY_PLUGINS_FALSE)v1_0_0 v1_1_0 v1_2_0 v1_3_0
 MODERN_PLUGINS = v2_0_0 v2_0_1 v2_0_4 v2_0_9 v2_0_10 v2_0_11 v2_0_12 v2_0_13 v2_0_14 \
                  v2_0_15 v2_1_1 v2_1_8
    ALL_PLUGINS = $(LEGACY_PLUGINS) $(MODERN_PLUGINS) 
PLUGIN_PACKAGES = $(sort $(patsubst %,us.temerity.pipeline.plugin.%,$(ALL_PLUGINS)))


  MAIN_PACKAGES = us.temerity.pipeline \
                  us.temerity.pipeline.event \
                  us.temerity.pipeline.glue \
                  us.temerity.pipeline.glue.io \
                  us.temerity.pipeline.math \
                  us.temerity.pipeline.toolset \
                  us.temerity.pipeline.ui


       PACKAGES = $(MAIN_PACKAGES) $(PLUGIN_PACKAGES)

   PACKAGE_HTML = $(patsubst %,$(top_srcdir)/src/java/%/package.html,\
                             $(subst .,/,$(PACKAGES)))


   JAVADOC_OPTS = -sourcepath $(top_srcdir)/src/java \
                  -classpath $(top_builddir)/src/java:$(srcdir)/../../src/java/jogl/jogl.jar \
                  -d javadoc -source 1.5 -use $(EXTERNAL_URLS) \
                  -header '</EM><BIG><A href="http://www.temerity.us" target="_top"><SPAN style="color: \#ffffff;"><B>TEMERITY</B></SPAN></A></BIG><EM>' \
                  -stylesheetfile $(srcdir)/stylesheet.css


#$(LOCAL_URLS) 
#$(EXTERNAL_URLS) 

#--------------------------------------------------------------------------------------------#
#   R U L E S                                                                                #
#--------------------------------------------------------------------------------------------#

all: javadoc.tar

javadoc.tar: docs-all
	tar -cvf $@ javadoc

docs-all: $(PACKAGE_HTML) Makefile
	$(JAVADOC) $(JAVADOC_OPTS) $(PACKAGES) \
           $(top_builddir)/src/java/us/temerity/pipeline/PackageInfo.java
	cp $(srcdir)/inherit.gif javadoc/resources/inherit.gif
	touch $@


#--------------------------------------------------------------------------------------------#

install: all installdirs
	(docdir=`pwd`; \
         cd $(DESTDIR)$(ROOT_INST_DIR)/share/docs; \
         tar -xvf $$docdir/javadoc.tar)

PHONY: installdirs
installdirs:
	$(mkinstalldirs) $(DESTDIR)$(ROOT_INST_DIR)/share/docs


#--------------------------------------------------------------------------------------------#

uninstall: 

uninstall-jar: 
	rm -f $(DESTDIR)$(ROOT_INST_DIR)/share/docs/javadoc


#--------------------------------------------------------------------------------------------#

check: all


#--------------------------------------------------------------------------------------------#

clean: 
	rm -rf javadoc
	rm -f javadoc.tar
	rm -f docs-all


#--------------------------------------------------------------------------------------------#

Makefile: $(srcdir)/Makefile.in  $(top_builddir)/config.status
	cd $(top_builddir) && $(SHELL) ./config.status $(subdir)/$@

